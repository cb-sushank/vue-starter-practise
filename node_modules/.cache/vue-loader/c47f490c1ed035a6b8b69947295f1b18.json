ag=\"li\" :class=\"member.id < members.length -1 && 'border-bottom  border-secondary'\">\n              <template #aside>\n                <b-avatar\n                  width=\"64\"\n                  alt=\"placeholder\"\n                  src=\"https://picsum.photos/125/125/?image=58\"\n                ></b-avatar>\n\n                <div class=\"member-info\">\n                  <h5 class=\"mt-0 mb-1\">{{member.name }}</h5>\n                  <p class=\"mb-0\">\n                    {{ member.email }}\n                  </p>\n                  <p class=\"mb-0\">\n                    {{ member.role }}\n                  </p>\n                </div>\n                <div class=\"member-change\">\n                  <button style=\"\" v-on:click='editMemberButton(email)'>Edit</button>\n                  <button style=\"margin-left:20px\" v-on:click='removeMemberButton(member.email)'>Remove</button>\n                </div>\n              </template>\n            </b-media>\n          </ul>\n        </b-card>\n      </div>\n    </body>\n  </html>\n</template>\n\n<script lang=\"ts\">\nimport Vue from 'vue';\nimport Component from 'vue-class-component';\n\nVue.component(\"modal\", {\n        template: \"#modal-template\"\n      });\n\n      // start app\n      new Vue({\n        el: \"#app\",\n        data: {\n          showModal: false\n        }\n      });\n@Component({\n  name: 'TeamMemberList',\n  mounted(){\n    document.title=\"Team Management\";\n  },\n  props: {\n    members: {\n      type: Array,\n      default: () => [],\n    },\n  },\n  methods: {\n    addMemberModal(){\n      \n    },\n    editMemberButton(email){\n      if (confirm(`Do you want to edit ${email}?`) === true) {\n        this.$store.commit('editMember', email);\n      }\n    },\n    removeMemberButton(email) {\n      if (confirm(`Do you want to remove ${email}?`) === true) {\n        this.$store.commit('removeMember', email);\n      }\n    },\n  },\n})\nexport default class TeamMemberList extends 